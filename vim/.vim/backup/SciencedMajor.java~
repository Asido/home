public class SciencedMajor implements Student {
	private char matLevel;
	private String name;
	private String phone;
	private int germanGrade;

	public SciencedMajor(String name, String phone) {
		this.name = name;
		this.phone = phone;
	}

	public boolean passed() {
		if(germanGrade >= 02)
			return true;
		return false;
	}

	public void setMatLevel(char matLevel) {
		this.matLevel = matLevel;
	}

	public void setGermanGrade(int germanGrade) {
		this.germanGrade = germanGrade;
	}

	public char getMatLevel() {
		return matLevel;
	}

	public int getGermanGrade() {
		return germanGrade;
	}

	public String getPhone() {
		return phone;
	}

	public String getName() {
		return name;
	}
}

public class SavingAccount extends BankAccount {
	private int cancledAmount;
	private int time;

	public SavingAccount(int cancledAmount, int time) {
		super(3000, 2);
		this.cancledAmount = cancledAmount;
		this.time = time;
	}

	public boolean withdraw(int amount) {
		if(amount <= cancledAmount) {
			setBalance(getBalance() - amount);
			return true;
		}
		return false;
	}

	public int getAvailableBalance() {
		return cancledAmount;
	}
}

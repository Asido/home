#include <fcntl.h>
#include "../../../source/lib/tlpi_hdr.h"

int main(int argc, char* argv[])
{
	if (argc != 2 || strcmp(argv[1], "--help") == 0)
		usageErr("%s file\n", argv[0]);

	off_t fd = open(argv[1], O_WRONLY | O_APPEND);
	lseek(fd, 0, SEEK_SET);
	write(fd, "append", 6);
	close(fd);

	exit(EXIT_SUCCESS);
}

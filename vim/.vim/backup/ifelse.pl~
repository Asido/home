#!/usr/bin/perl -w

print "Enter a number: ";
$num1 = <>;
print "Enter another, different number: ";
$num2 = <>;

if($num2 == $num2) {
	die("Please enter two different numbers.\n");
}
elsif($num1 > $num2) {
	print "The first number is greater than the second one.\n";
}
else {
	print "The second number is greater than the first one.\n";
}

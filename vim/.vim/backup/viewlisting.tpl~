<script type="text/javascript" src="templates/mn/scripts/jquery.js"></script>
<link rel="stylesheet" type="text/css" href="templates/mn/css/layout/jquery.ad-gallery.css">
  <script type="text/javascript" src="templates/mn/scripts/jquery.ad-gallery.js"></script>
  <script type="text/javascript">
 $(function() {
	 
   jQuery('img.image0').data('ad-desc', 'name of the image');
   jQuery('img.image1').data('ad-desc', 'name of the image');
    jQuery('img.image2').data('ad-desc', 'name of the image');
    jQuery('img.image3').data('ad-desc', 'name of the image');
	jQuery('img.image4').data('ad-desc', 'name of the image');
	jQuery('img.image5').data('ad-desc', 'name of the image');
	jQuery('img.image6').data('ad-desc', 'name of the image');
	jQuery('img.image7').data('ad-desc', 'name of the image');
	jQuery('img.image8').data('ad-desc', 'name of the image');
    var galleries = $('.ad-gallery').adGallery();
    jQuery('#switch-effect').change(
      function() {
        galleries[0].settings.effect = $(this).val();
        return false;
      }
    );
    jQuery('#toggle-slideshow').click(
      function() {
        galleries[0].slideshow.toggle();
        return false;
      }
    );
    jQuery('#toggle-description').click(
      function() {
        if(!galleries[0].settings.description_wrapper) {
          galleries[0].settings.description_wrapper = $('#descriptions');
        } else {
          galleries[0].settings.description_wrapper = false;
        }
        return false;
      }
    );
  });
  </script>
{/literal}



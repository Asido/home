public class Launcher {
	public static void main(String[] args) {
		BankAccount account1 = new BudgetAccount(500);
		BankAccount account2 = new SavingAccount(1200, 2);
		BankAccount account3 = new PersonalAccount(678);

		Customer customer = new Customer("Arvydas", "Oster Uttrup Vej 3", "9000", "Aalborg");
		customer.addAccount(account1);
		customer.addAccount(account2);
		customer.addAccount(account3);
		customer.printTotalBalance();
		customer.printAvailableBalance();

		FileHandler fileHandler = new FileHandler("customerData");
		try {
			fileHandler.writeToFile(customer);
			Customer c = (Customer)fileHandler.readFromFile();
			c.printTotalBalance();
			c.printAvailableBalance();
		} catch(Exception e) {
			System.err.println(e.getMessage());
		}
	}
}

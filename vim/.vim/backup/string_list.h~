#ifndef STRING_LIST_KGHP3XFZ
#define STRING_LIST_KGHP3XFZ

#include <string>
#include <list>
#include <iostream>

class string_list {
	public:
		typedef std::list<std::string>::iterator iterator;
		typedef std::list<std::string>::const_iterator const_iterator;

		void push_back(std::string str)
		{
			std::cout << "!!!" << std::endl;
			s.push_back(str);
		}

		iterator begin()
		{
			return s.begin();
		}
		const_iterator begin() const
		{
			return s.begin();
		}

		iterator end()
		{
			return s.end();
		}
		const_iterator end() const
		{
			return s.end();
		}
		
	private:
		std::list<std::string> s;
};

#endif /* end of include guard: STRING_LIST_KGHP3XFZ */

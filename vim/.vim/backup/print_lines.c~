#include <stdio.h>

int getLn(char line[], int max_limit);
int printLine(char line[]);

/*
 * Prints lines longer then 80 to std output.
 */
int main() {
	int char_limit = 20;
	int max_limit = 1000;
	int current_length;
	char line[max_limit];

	while((current_length = getLn(line, max_limit)) > 0) {
		if(current_length >= char_limit)
			printLine(line);
	}
}

int getLn(char line[], int size) {
	int c;
	int i;

	for(i = 0; (c = getchar()) != EOF && c != '\n' && i < (size - 1); i++)
		line[i] = c;

	if(c == '\n') {
		line[i] = c;
		i++;
	}
	line[i] = '\0';
	i++;
	return i;
}

int printLine(char line[]) {
	int i = 0;
	while(line[i] != '\0') {
		putchar(line[i]);
		i++;
	}
}

#ifndef PROCTREE_139PLTIG
#define PROCTREE_139PLTIG

struct proc {
	int pid;
	int ppid;
	char *cmd;
};

#define MAX_FILENAME_SIZE 100
#define MAX_ARG_SIZE 50

#define max(m,n) ((m) > (n) ? (m) : (n))
int proc_count();
int build_proc(struct proc *struct_buf[], int count);
void print_tree(struct proc *struct_buf[], int count);

#endif /* end of include guard: PROCTREE_139PLTIG */

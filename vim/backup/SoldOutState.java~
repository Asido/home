public class SoldOutState implements State {
	private GumballMachine gumballMachine;

	public SoldOutState(GumballMachine gumballMachine) {
		this.gumballMachine = gumballMachine;
	}

	public void insertQuarter() {
		System.out.println("There is no gumballs left");
		gumballMachine.ejectQuarter();
	}

	public void ejectQuarter() {
		System.out.println("Ejecting the quarter...");
	}

	public void turnCrank() {
		System.out.println("There is no gumballs left, can't you see?!");
	}

	public void dispense() {
		System.out.println("Cannot dispense. There is no gumballs left.");
	}

	public String toString() {
		return "all gumballs are sold out";
	}
}

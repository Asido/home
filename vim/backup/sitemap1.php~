		<div id="loc3">
			<div class="loc_head">
				 SHOP BY CATEGORY
			</div>
			<table width="560" cellspacing="0" cellpadding="0" border="0" align="center">
			<tbody>
            <?php
			$sqlfull="select * from wb_sitemap_cat where maincat='1' order by priority ASC";
			$sqlefull=mysql_query($sqlfull);
			$countfull=mysql_num_rows($sqlefull);
			if($countfull!=0)
			{
				$i=0;
				while($infofull=mysql_fetch_array($sqlefull))
				{
					$titlefull=$infofull['title'];
					$urlfull=$infofull['url'];
					$idfull=$infofull['id'];
					if($i==0)
					{
						echo '<tr>';
					}
					if($i<2)
					{
						echo '<td valign="top"><img src="../images/bullets/5.gif"><a class="sitemap_link bold" href="'.$urlfull.'">'.$titlefull.'</a><br><ul>';
						// listing
						$sqlfull1="select * from wb_sitemap where category='$idfull'";
						$sqlefull1=mysql_query($sqlfull1);
						$countfull1=mysql_num_rows($sqlefull1);
						if($countfull1!=0)
						{
							while($infofull1=mysql_fetch_array($sqlefull1))
							{
								$titlefull1=$infofull1['title'];
								$urlfull1=$infofull1['url'];
								echo '<li><img src="../images/bullets/2.gif"><a class="sitemap_link" href="'.$urlfull1.'">'.$titlefull1.'</a></li>';
							}
						}
						
						//listing
						
						
						echo '</ul></td>';
						$i++;
					}
					if($i==2)
					{
						echo '</tr>';
						$i=0;
					}
						
					
				}
			}
			?>
            
           
			
			</tbody>
			</table>
		</div>

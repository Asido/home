		<div id="loc2">
			<div class="loc_head">
				HOME
			</div>
			<table width="560" cellspacing="0" cellpadding="0" border="0" align="center">
			<tbody>
            <?php
			$sqlfull="select * from wb_sitemap_cat where maincat='2' order by priority ASC";
			$sqlefull=mysql_query($sqlfull);
			$countfull=mysql_num_rows($sqlefull);
			if($countfull!=0)
			{
				$i=0;
				while($infofull=mysql_fetch_array($sqlefull))
				{
					$titlefull=$infofull['title'];
					$urlfull=$infofull['url'];
					if($i==0)
					{
						echo '<tr>';
					}
					if($i<3)
					{
						echo '<td><img src="../images/bullets/5.gif"><a class="sitemap_link bold" href="'.$urlfull.'">'.$titlefull.'</a></td>';
						$i++;
					}
					if($i==3)
					{
						echo '</tr>';
						$i=0;
					}
						
					
				}
			}
			?>
            
		
          
			</tbody>
			</table>
		</div>

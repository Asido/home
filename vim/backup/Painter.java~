import java.util.ArrayList;

abstract class Shape
{
	public abstract void draw();
}


class Point extends Shape
{
	private int x, y;

	public Point(int x, int y)
	{
		this.x = x;
		this.y = y;
	}

	public void draw()
	{
		System.out.println("Drawing a point at (" + x + "; "+ y + ")...");
	}
}


class Line extends Shape
{
	private int x1, y1;
	private int x2, y2;

	public Line(int x1, int y1, int x2, int y2)
	{
		this.x1 = x1;
		this.y1 = y1;
		this.x2 = x2;
		this.y2 = y2;
	}

	public void draw()
	{
		System.out.println("Drawing a line beginning at ("+x1+"; "+y1+") and ending at ("+x2+"; "+y2+")...");
	}
}


class Circle extends Shape
{
	private int x, y;
	private double size;

	public Circle(int x, int y, double size)
	{
		this.x = x;
		this.y = y;
		this.size = size;
	}

	public void draw()
	{
		System.out.println("Drawing a circle at (" + x + "; " + y + ") of size " + size + " cm...");
	}
}


class Rectangle extends Shape
{
	private int x, y;
	private double size;

	public Rectangle(int x, int y, double size)
	{
		this.x = x;
		this.y = y;
		this.size = size;
	}

	public void draw()
	{
		System.out.println("Drawing a rectangle at (" + x + "; " + y + "), of size " + size + " cm...");
	}
}

class Cylinder extends Shape
{
	private int x, y;
	
	public Cylinder(int x, int y)
	{
		this.x = x;
		this.y = y;
	}

	public void draw()
	{
		System.out.println("Drawing a cylinder at (" + x + "; " + y + ")...");
	}
}

class Cube extends Shape
{
	private int x, y;
	private double size;

	public Cube(int x, int y, double size)
	{
		this.x = x;
		this.y = y;
		this.size = size;
	}

	public void draw()
	{
		System.out.println("Drawing a cube at (" + x + "; " + y + "), of size " + size + " cm...");
	}
}


//////////////////////////////////////////////////////////////////////////////
// MAIN CLASS

public class Painter
{
	public static void main(String[] args)
	{
		Shape shape1 = new Point(32, 43);
		Shape shape2 = new Line(2, 3, 14, 4);
		Shape shape3 = new Circle(50, 100, 30);
		Shape shape4 = new Rectangle(0, 0, 5);
		Shape shape5 = new Cylinder(10, 10);
		Shape shape6 = new Cube(-44, 12, 15);

		ArrayList<Shape> shapeContainer = new ArrayList<Shape>();
		shapeContainer.add(shape1);
		shapeContainer.add(shape2);
		shapeContainer.add(shape3);
		shapeContainer.add(shape4);
		shapeContainer.add(shape5);
		shapeContainer.add(shape6);

		for (Shape shape : shapeContainer)
			shape.draw();
	}
}

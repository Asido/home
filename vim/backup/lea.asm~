;	Build:
;		nasm -f elf -g -F stabs uppercaser.asm
;		ld -o uppercaser uppercaser.o

SECTION .data
	ShiftMe: dd 1234h, 5678h, 9012h
SECTION .bss
SECTION .text

global _start

_start:
	nop
	
	mov eax, [ShiftMe]
	mov edx, [ShiftMe+8]
	mov cl, 1
	shld edx, eax, cl
	wbinvd

	; clean exit
	mov eax, 1
	mov ebx, 0
	int 0x80

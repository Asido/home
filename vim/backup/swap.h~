#ifndef GUARD_swap_h
#define GUARD_swap_h


template <class Bi> void my_reverse(Bi begin, Bi end) {
	while(begin != end) {
		--end;
		if(begin != end) {
			swap(*begin++, *end);
		}
	}
}
template <class Bi> void swap(Bi& first, Bi& second) {
	Bi temp = first;
	first = second;
	second = temp;
}

#endif

# To unbundle, bash this file
if [[ $# = 0 || $* = *en* ]]; then
echo en 1>&2
cat >en <<'End of en'
hello
i am arvydas
i am a student
how are you guys?
End of en
End of en
fi
if [[ $# = 0 || $* = *fr* ]]; then
echo fr 1>&2
cat >fr <<'End of fr'
bonjour
vi va deliva
End of fr
fi
if [[ $# = 0 || $* = *lt* ]]; then
echo lt 1>&2
cat >lt <<'End of lt'
sveiki
kaip einas?
man tai gerai
studinu danijoj
ir visai patinka
End of lt
fi

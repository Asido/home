#!/bin/bash

NAMES=(max helen zach bob)

# '*' gives a single element with all the values
# separated with the first value in IFS (usualy a space)
A=("${NAMES[*]}")
# '@' gives a copy of an array.
B=("${NAMES[@]}")

echo

echo "${A[0]}"
echo "${B[0]}"

echo

echo "${A[1]}"
echo "${B[1]}"

echo

echo "${#NAMES[*]}"
echo "${#NAMES[1]}"

var="KOJIYGVIKJHUJKHKJHJKMJ"
echo "${#var}"
echo "${var}"
